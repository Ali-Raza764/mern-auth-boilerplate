{
  "version": 2,
  "builds": [
    {
      "src": "/backend/src/index.mjs",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm install && npm run build-client && npm run build-server"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/src/index.mjs"
    }
  ]
}
